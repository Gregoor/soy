{
  "name": "soyfu",
  "displayName": "Soy",
  "version": "1.0.0",
  "description": "Structural commands based on your code's syntax tree",
  "publisher": "watware",
  "bugs": {
    "url": "https://github.com/gregoor/soy",
    "email": "gregorwbr@gmail.com"
  },
  "license": "UNLICENSED",
  "scripts": {
    "build": "ts-node scripts/cp-wasm.ts && ts-node scripts/esbuild.ts",
    "watch": "npm run build --  --dev",
    "types": "tsc --noEmit",
    "test": "jest src"
  },
  "engines": {
    "vscode": "^1.74.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@swc/jest": "^0.2.24",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.18",
    "@types/vscode": "^1.74.0",
    "esbuild": "^0.16.12",
    "fs-extra": "^11.1.0",
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
    "tree-sitter-javascript": "^0.19.0",
    "tree-sitter-wasms": "^0.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "web-tree-sitter": "^0.20.7"
  },
  "main": "./out/node.js",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:c",
    "onLanguage:cpp",
    "onLanguage:csharp",
    "onLanguage:css",
    "onLanguage:go",
    "onLanguage:html",
    "onLanguage:java",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:json",
    "onLanguage:python",
    "onLanguage:ruby",
    "onLanguage:rust",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:yaml"
  ],
  "contributes": {
    "commands": [
      {
        "command": "soy.enterSelectionMode",
        "title": "Soy: Enter Selection Mode"
      },
      {
        "command": "soy.leaveSelectionMode",
        "title": "Soy: Leave Selection Mode"
      },
      {
        "command": "soy.extendSelection",
        "title": "Soy: Extend Selection"
      },
      {
        "command": "soy.shrinkSelection",
        "title": "Soy: Shrink Selection"
      },
      {
        "command": "soy.selectPreviousNeighbor",
        "title": "Soy: Select Previous Neighbor"
      },
      {
        "command": "soy.selectNextNeighbor",
        "title": "Soy: Select Next Neighbor"
      },
      {
        "command": "soy.unwrap",
        "title": "Soy: Unwrap"
      },
      {
        "command": "soy.moveLeft",
        "title": "Soy: Move Left"
      },
      {
        "command": "soy.moveRight",
        "title": "Soy: Move Right"
      }
    ],
    "keybindings": [
      {
        "command": "soy.enterSelectionMode",
        "key": "Shift Shift",
        "when": "editorTextFocus && !editorHasMultipleSelections"
      },
      {
        "command": "soy.leaveSelectionMode",
        "key": "Escape",
        "when": "editorTextFocus && soy.isSelectionMode"
      },
      {
        "command": "soy.extendSelection",
        "key": "Shift+Alt+up",
        "when": "editorTextFocus && !editorHasMultipleSelections"
      },
      {
        "command": "soy.shrinkSelection",
        "key": "Shift+Alt+down",
        "when": "editorTextFocus && !editorHasMultipleSelections"
      },
      {
        "command": "soy.extendSelection",
        "key": "up",
        "when": "editorTextFocus && !editorHasMultipleSelections && soy.isSelectionMode"
      },
      {
        "command": "soy.shrinkSelection",
        "key": "down",
        "when": "editorTextFocus && !editorHasMultipleSelections && soy.isSelectionMode"
      },
      {
        "command": "soy.unwrap",
        "key": "Ctrl+Shift+up",
        "when": "editorTextFocus && !editorHasMultipleSelections"
      },
      {
        "command": "soy.moveLeft",
        "key": "Ctrl+Shift+left",
        "when": "editorTextFocus && !editorHasMultipleSelections"
      },
      {
        "command": "soy.moveRight",
        "key": "Ctrl+Shift+right",
        "when": "editorTextFocus && !editorHasMultipleSelections"
      }
    ]
  }
}
